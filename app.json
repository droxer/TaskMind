{
  "expo": {
    "name": "TaskMind",
    "slug": "taskmind",
    "scheme": "taskmind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1726"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taskmind.app",
      "usesIcloudStorage": true,
      "infoPlist": {
        "NSUbiquitousContainers": {
          "iCloud.com.taskmind.app": {
            "NSUbiquitousContainerIsDocumentScopePublic": true,
            "NSUbiquitousContainerSupportedFolderLevels": "None",
            "NSUbiquitousContainerName": "TaskMind",
            "NSUbiquitousContainerEnabled": true
          }
        },
        "NSUbiquitousContainerIdentifiers": [
          "iCloud.com.taskmind.app"
        ],
        "NSUbiquitousContainerName": "TaskMind",
        "NSUbiquitousContainerSupportedFolderLevels": "None",
        "NSUbiquitousContainerIsDocumentScopePublic": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0E1726"
      },
      "package": "com.taskmind.app"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "CHANGE-ME"
      }
    }
  }
}
